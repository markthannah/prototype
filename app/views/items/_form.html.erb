      toggleFields();
      $("#jewelrytype").change(function () {
          toggleFields();
      });

  });
  function toggleFields() {

      if ($("#jewelrytype").val() === "bracelets")
          $("#braceletdetails").show();
      else
          $("#braceletdetails").hide();

      if ($("#jewelrytype").val() === "brooches")
          $("#broochdetails").show();
      else
          $("#broochdetails").hide();

      if ($("#jewelrytype").val() === "cufflinks")
